{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dotnet",
			"task": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"label": "dotnet: build"
		},
		{
			"type": "process",
			"command": "dotnet",
			"problemMatcher": [
				"$msCompile"
			],
			"args": [
				"build",
				"-c",
				"Release",
				"-o",
				"../Runtime"
			],
			"label": "dotnet: build release"
		}
	]
}